{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "target": "es6",
    "jsx": "preserve",
    "noImplicitAny": false,
    "importHelpers": true,
    "moduleResolution": "node",
    "strictNullChecks": false,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "sourceMap": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "dist"]
}
